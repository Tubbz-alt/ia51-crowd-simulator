 * @author LÃ©on
 * Any object with a position
 */
interface ISituatedObject extends IShapedObject, Cloneable, Comparable<ISituatedObject>{
	
	def getPosition() : Point2d
	
	def getX() : double
	
	def getY() : double
	
	def getId() : UUID

	def getType : Serializable
	
	def clone : ISituatedObject
	
}
