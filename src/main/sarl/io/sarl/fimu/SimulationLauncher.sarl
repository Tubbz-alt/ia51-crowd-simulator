package io.sarl.fimu

import io.janusproject.Boot
import io.sarl.fimu.agents.SimulationAgent
import io.sarl.fimu.gui.FXMLViewerController

class SimulationLauncher {
	static def main(controller : FXMLViewerController) {
		Boot::offline = true
		Boot::verboseLevel = 7
		Boot::startJanus(typeof(SimulationAgent), controller)
	}
}
